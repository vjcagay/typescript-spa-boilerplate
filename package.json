{
  "name": "webpack-typescript",
  "version": "1.1.0",
  "description": "Webpack configuration for TypeScript projects",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "compile:development:dll": "webpack --config webpack.config.dll.ts --mode=development",
    "compile:development:source": "webpack --mode=development",
    "compile:production:dll": "webpack --config webpack.config.dll.ts --mode=production",
    "compile:production:source": "webpack --mode=production",
    "start": "webpack-dev-server --mode=development",
    "tslint": "tslint --project ./"
  },
  "keywords": [
    "TypeScript",
    "WebPack",
    "configuration",
    "module",
    "bundler"
  ],
  "author": "Vincent John Cagay",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/autoprefixer": "^9.5.0",
    "@types/clean-webpack-plugin": "^0.1.3",
    "@types/compression-webpack-plugin": "^2.0.0",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/jest": "^24.0.15",
    "@types/mini-css-extract-plugin": "^0.2.1",
    "@types/node": "^12.0.10",
    "@types/optimize-css-assets-webpack-plugin": "^1.3.4",
    "@types/webpack": "^4.4.33",
    "@types/webpack-dev-server": "^3.1.6",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "autoprefixer": "^9.6.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.0.0",
    "css-loader": "^3.0.0",
    "eslint": "^6.0.0",
    "file-loader": "^4.0.0",
    "html-webpack-include-assets-plugin": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
