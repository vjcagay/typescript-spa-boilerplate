{
  "name": "webpack-typescript-styled-components",
  "version": "1.1.0",
  "description": "Webpack configuration for TypeScript projects",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "compile:development:dll": "webpack --config webpack.config.dll.ts --mode=development",
    "compile:development:source": "webpack --mode=development",
    "compile:production:dll": "webpack --config webpack.config.dll.ts --mode=production",
    "compile:production:source": "webpack --mode=production",
    "start": "webpack-dev-server --mode=development",
    "tslint": "tslint --project ./"
  },
  "keywords": [
    "TypeScript",
    "WebPack",
    "configuration",
    "module",
    "bundler"
  ],
  "author": "Vincent John Cagay",
  "license": "MIT",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.3.2",
    "styled-reset": "^2.0.16"
  },
  "devDependencies": {
    "@types/clean-webpack-plugin": "^0.1.3",
    "@types/compression-webpack-plugin": "^2.0.0",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.10",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/styled-components": "^4.1.16",
    "@types/webpack": "^4.4.33",
    "@types/webpack-dev-server": "^3.1.6",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.0.0",
    "eslint": "^6.0.0",
    "file-loader": "^4.0.0",
    "html-webpack-include-assets-plugin": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
