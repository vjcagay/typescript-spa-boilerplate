{
  "name": "typescript-spa-boilerplate",
  "version": "4.0.1",
  "description": "Webpack configuration for TypeScript projects",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "dll": "webpack --config webpack.config.dll.ts",
    "source": "webpack",
    "start": "webpack serve",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [
    "TypeScript",
    "WebPack",
    "configuration",
    "module",
    "bundler"
  ],
  "author": "Vincent John Cagay",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.0",
    "styled-reset": "^4.2.3"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^12.1.2",
    "@types/clean-webpack-plugin": "^0.1.3",
    "@types/compression-webpack-plugin": "^9.1.1",
    "@types/html-webpack-plugin": "^3.2.3",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.15",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/styled-components": "^5.1.4",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^9.2.0",
    "eslint": "^8.8.0",
    "html-webpack-plugin": "^5.2.0",
    "html-webpack-tags-plugin": "^3.0.0",
    "jest": "^27.4.7",
    "react-refresh": "^0.11.0",
    "react-refresh-typescript": "^2.0.3",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "type-fest": "^2.11.1",
    "typescript": "^4.0.3",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^4.7.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vjcagay/typescript-spa-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/vjcagay/typescript-spa-boilerplate/issues"
  },
  "homepage": "https://github.com/vjcagay/typescript-spa-boilerplate#readme"
}
