{
  "name": "typescript-spa-boilerplate",
  "version": "3.0.0",
  "description": "Webpack configuration for TypeScript projects",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "compile:development:dll": "webpack --config webpack.config.dll.ts --mode=development",
    "compile:development:source": "webpack --mode=development",
    "compile:production:dll": "webpack --config webpack.config.dll.ts --mode=production",
    "compile:production:source": "webpack --mode=production",
    "start": "webpack-dev-server --mode=development",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [
    "TypeScript",
    "WebPack",
    "configuration",
    "module",
    "bundler"
  ],
  "author": "Vincent John Cagay",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.0",
    "styled-reset": "^4.2.3"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@types/clean-webpack-plugin": "^0.1.3",
    "@types/compression-webpack-plugin": "^6.0.4",
    "@types/html-webpack-plugin": "^3.2.3",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.10",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/styled-components": "^5.1.4",
    "@types/webpack": "^4.41.22",
    "@types/webpack-dev-server": "^3.11.0",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^6.0.3",
    "eslint": "^7.11.0",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^4.5.0",
    "html-webpack-tags-plugin": "^2.0.17",
    "jest": "^26.5.3",
    "react-refresh": "^0.9.0",
    "react-refresh-typescript": "^1.0.1",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.5",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vjcagay/webpack-typescript-styled-components.git"
  },
  "bugs": {
    "url": "https://github.com/vjcagay/webpack-typescript-styled-components/issues"
  },
  "homepage": "https://github.com/vjcagay/webpack-typescript-styled-components#readme"
}
